{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "eol-last": 1,
        "eqeqeq": 2,
        "dot-notation": 1,
        "indent": [1, 4],
        "no-console": 0,
        "no-loop-func": 1,
        "no-unused-vars": 1,
        "quotes": [1, "single"],
        "semi": 2
    }
}
